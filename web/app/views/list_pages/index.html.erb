<h1>Listing list_pages</h1>
<%= link_to "CSV", list_pages_path(format: "csv")%>
<table>
  <tr>
    <th>Url</th>
    <th>Page</th>
    <th>Min value</th>
    <th>Max value</th>
  </tr>

<% @list_pages.each do |list_page| %>
  <tr>
    <td><%= list_page.url %></td>
    <td><%= list_page.page %></td>
    <td><%= list_page.min_value %></td>
    <td><%= list_page.max_value %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New List page', new_list_page_path %>
